{
  "name": "@sagua/worker",
  "type": "module",
  "scripts": {
    "format": "prettier -w --plugin-search-dir=. src",
    "clean": "del-cli ./dist",
    "build": "swc ./src -d ./dist -q",
    "watch": "chokidar \"./src/**/*\" -c \"pnpm build\" --silent --initial",
    "start:prod": "node ./dist/index.js"
  },
  "dependencies": {
    "@sagua/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.3.1"
  }
}